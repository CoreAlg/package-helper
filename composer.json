{
    "name": "corealg/helper",
    "description": "This package provides some common helper functions that we need regularly for almost every project.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "helper"
    ],
    "authors": [
        {
            "name": "Mizanur Rahman",
            "email": "mizan3008@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ntwindia/ntwindia": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CoreAlg\\Helper\\": "src"
        }
    },
    "minimum-stability": "dev",
    "autoload-dev": {
        "psr-4": {
            "CoreAlg\\Helper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    }
}